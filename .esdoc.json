{
  "source": "./src",
  "destination": "./docs",
  "excludes": [ ".+\\.test\\.js" ], 
  "package": "./package.json", 
  "plugins": [
    {
      "name": "esdoc-ecmascript-proposal-plugin", 
      "option": { 
        "all": true
      }
    },
    {
      "name": "esdoc-standard-plugin",
      "option": {
        "lint": { 
          "enable": false 
        }
      }
    },
    {
      "name": "esdoc-importpath-plugin",
      "option": {
        "replaces": [
          { "from": ".*", "to": "" }
        ]
      }
    }
  ]
}
